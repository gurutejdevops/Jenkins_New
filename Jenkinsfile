pipeline {
    agent any

    stages {
        stage('stage one') {
            steps {
                sh '''
                echo stage one 
                echo welcome to Jenkins
                '''
            }
        }
        stage('stage two') {
            steps {
                sh "echo stage two "
            }
        }
        stage('stage three') {
            steps {
                sh "echo stage three "
            }
        }
    }
}
